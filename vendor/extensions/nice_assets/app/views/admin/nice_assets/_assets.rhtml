<h4 class="asset-tab"><span>Asset Clipboard</span></h4>
<div id="asset-actions-list-wrapper">
	<ul id="asset-actions-list">
		<li><span id="asset-upload">upload files</span></li>
    <!-- <li><span id="asset-search">search</span></li> -->
		<li><span id="asset-browse">browse files</span></li>
		<li><span id="asset-browse-pages">browse pages</span></li>
	</ul>
</div>
<div id="asset-wrapper">
	<div id="page-assets" class="all-assets">
		<%= render :partial => 'admin/nice_assets/asset_thumbnail', :collection => @page.all_assets, :locals => {:add_msg => "Add to this page at cursor position", :delete_msg => 'Remove from the asset clipboard'} %>
		<div class="clear"></div>
	</div>
</div>
<div id="removed-assets"></div>
